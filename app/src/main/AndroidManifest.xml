<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kogo.hastarandevusistemi">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/zonezero"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/zonezero"
        android:supportsRtl="true"
        android:theme="@style/Theme.HastaRandevuSistemi">
        <activity
            android:name=".AppointmentScreen"
            android:exported="false" />
        <activity
            android:name=".PaymentScreen"
            android:exported="false" />
        <activity
            android:name=".DoctorDetails"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>